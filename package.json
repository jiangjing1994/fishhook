{
  "name": "@jiangjing94/mimi",
  "version": "1.3.14",
  "private": false,
  "scripts": {
    "bootstrap": "yarn --frozen-lockfile && yarn gen:version",
    "gen:version": "node build/gen-version.js",
    "start": "npm-run-all --parallel start:*",
    "start:main": "yarn serve || npm run serve",
    "start:docs": "yarn docs || npm run docs",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:prettier": "prettier --write --loglevel warn \"**/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lib": "vue-cli-service build --target lib --name index --dest lib packages/index.js",
    "new": "plop packages",
    "docs": "vue-cli-service styleguidist",
    "docs:build": "vue-cli-service styleguidist:build",
    "fast-build": "sh ./scripts/build.sh"
  },
  "main": "lib/index.umd.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "lib/index.umd.min.js",
    "lib/index.css",
    "lib/fonts",
    "lib/img",
    "packages"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@smallwei/avue": "2.6.7",
    "axios": "0.19.2",
    "core-js": "3.6.5",
    "countup.js": "1.9.3",
    "element-ui": "2.13.2",
    "lodash": "4.17.19",
    "pug": "2.0.3",
    "pug-html-loader": "1.1.5",
    "pug-plain-loader": "1.0.0",
    "view-design": "4.5.0",
    "vue": "2.6.11",
    "vue-router": "3.2.0",
    "vuex": "3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "plop": "2.3.0",
    "prettier": "^2.2.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-styleguidist": "~4.35.0",
    "vue-template-compiler": "2.6.11"
  },
  "license": "MIT"
}
